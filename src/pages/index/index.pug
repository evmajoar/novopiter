          +wrapper()( class=presentation.bodyContainer.class )
            +list()( class=presentation.list.class + " " + sliderClass )
              +list-item( class=presentation.listItem.class )
                +title( "От собственников - торг", "h4" )( class=presentation.listItemTitle.class )
                +text( "На такие квартиры всегда уместен торг, аргументированный торг может сильно снизить стоимость и " +
                "зачастую это самые интересные квартиры в комплексе" )
              +list-item( class=presentation.listItem.class )
                +title( "Переуступки - дешевле", "h4" )( class=presentation.listItemTitle.class )
                +text( "Переуступки могут продавать по разным причинам, зачастую из-за трудностей с выплатой ипотеки, " +
                "такие квартиры дешевле на 7-15% от цены застройщика" )
              +list-item( class=presentation.listItem.class )
                +title( "От застройщика", "h4" )( class=presentation.listItemTitle.class )
                +text( "На такие квартиры возможен максимальные бонусы от «Новостроек Петербурга». Ниже можно рассчитать " +
                "размер кэшбэка в зависимости от ЖК и цены квартиры " )
              +list-item( class=presentation.listItem.class )
                +title( "От подрядчика - самые выгодные!", "h4" )( class=presentation.listItemTitle.class )
                +text( "Пул квартиры, которыми застройщик расплачивается с подрядчиками. Такие квартиры дешевле на 9-17% " +
                "застройщика, на них очень высокий спрос, особенно у инвесторов" )
            +wrapper()( class=slider.controls.class + " " + presentation.controls.class )
              +arrowPrev( presentation.arrow.class + " " + presentation.arrow.modButtonPrev )
              +dotsContainer( presentation.dots.class )
              +arrowNext( presentation.arrow.class + " " + presentation.arrow.modButtonNext )
    +wrapper()( class=presentation.note.class )
      +text( "Мы презентуем вам все квартиры в интересующих вас комплексах, согласуем лучшие условия приобретения, " +
      "поможем собрать пакет документов и одобрить ипотеку" )( class=presentation.noteText.class )
      +href( "Получить презентацию" )( class=presentation.noteButton.class, href="#" )


  //- Кешбэк
  include ../../sections/cashback/cashback.pug
  +section( "section" )( class=cashbackClass )
    +wrapper()( class=cashback.container.class )
      +title( "Вернем до 6% <br> от стоимости твоей квартиры", "h2" )( class=cashback.title.class )
      +text( "Выбери любую квартиру в новостройке с бесплатным сервисом поиска <br> недвижимости" +
      " «Новостройки <br> Петербурга» и получи кэшбэк до 6% или сертификат до 450 000р." )( class=cashback.text.class )
      +wrapper( "p" )( class=cashback.buttons.class )
        +button( "Получить презентацию", "blue" )( class=cashback.button.class )
        +button( "Как получить кэшбэк", "red" )( class=cashback.button.class )
          +icon( "play", 11, 16, "play" )


  //- Преимущества кэшбэка
  include ../../sections/advantages/advantages.pug
  +section( "section" )( class=advantagesCashbackClass )
    +wrapper()( class=advantages.container.class )
      +title( "Преимущества кешбэка", "h2" )( class=advantages.title.class )
      +list()( class=advantages.list.class )
        +list-item()( class=advantages.listItem.class )
          +title( "Напрямую с продавцом", "h3" )( class=advantages.subTitle.class )
            +icon( "seller", "32", "32" )
          +text( "Условия приобретения и цену вы обсуждаете напрямую" +
          " с продавцом, без посредников" )( class=advantages.text.class )
        +list-item()( class=advantages.listItem.class )
          +title( "На любую квартиру", "h3" )( class=advantages.subTitle.class )
            +icon( "money", "32", "32" )
          +text( "Выбери с нами любую квартиру ( от застройщика, собственника" +
          " или подрядчика) в новостройке и получи на нее кэшбэк до 6%" )( class=advantages.text.class )
        +list-item()( class=advantages.listItem.class )
          +title( "Большие бонусы", "h3" )( class=advantages.subTitle.class )
            +icon( "fireworks", "32", "32" )
          +text( "Эксклюзивные рекламные интеграции со спонсорами позволяют" +
          " давать нашим клиентам щедрые бонусы и разыгрывать квартиры" )( class=advantages.text.class )




  //- Калькулятор
  +wrapper()( class=pageMain.wrapper.class )

    //- Калькулятор
    include ../../sections/calculator/calculator.pug
    +section()( class=calculatorClass )
      +wrapper()( class=calculator.container.class )
        +title( "Рассчитайте сумму кэшбэка или сертификата", "h2" )( class=calculator.title.class )
        +form()( class=calculator.form.class )
          +fieldset( "Калькулятор" )
            +field-text({
              title: "Название ЖК",
              attrs: {
                placeholder: "Введите название комплекса"
