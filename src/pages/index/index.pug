//- Главная страница

extends ../../layouts/layout.pug

block meta

  //- Метаданные
  -
    var meta = {
      title: "Сео заголовок сайта",
      description: "Описание сайта",
      siteURN: "index.html"
    }

  title=meta.title + " | " + setting.siteName
  +meta( "description", meta.description )

  //- Разметка Open Graph
  +meta( "og:title", meta.title, false )
  +meta( "og:description", meta.description, false )
  +meta( "og:type", "website", false )
  +meta( "og:url", setting.siteURL + meta.siteURN, false )
  +meta( "og:locale", setting.lang + '_' + setting.lang.toUpperCase(), false )
  +meta( "og:image", setting.imagesSrc + "poster.png", false )
  +meta( "og:image:type", "image/png", false )
  +meta( "og:image:width", "512", false )
  +meta( "og:image:height", "512", false )
  +meta( "og:site_name", setting.siteName, false )

  //- Разметка Twitter Card
  +meta( "twitter:card", "summary" )
  +meta( "twitter:title", meta.title )
  +meta( "twitter:description", meta.description )
  +meta( "twitter:image", setting.imagesSrc + "poster.png" )
  +meta( "twitter:site", setting.siteName )

block prepend head
  +link( "canonical", setting.siteURL + meta.siteURN )

block append head

block header

  //- Верхний бар
  +wrapper()( class=pageHeader.topBar.class )

    //- Логотип
    include ../../blocks/logo/logo.pug
    +logo()( class=pageHeader.logo.class )
      +image( "logo.svg", "Логотип " + setting.siteName, "Логотип " + setting.siteName, 113, 33 )

    //- Основная навигация
    include ../../blocks/main-nav/main-nav.pug
    +main-nav()( class=pageHeader.mainNav.class )
      +list()( class=mainNav.list.class )
        +list-item()( class=mainNav.listItem.class )
          +href( "Подбор квартиры" )( class=mainNav.link.class, href="#" )
        +list-item()( class=mainNav.listItem.class )
          +href( "Наши услуги" )( class=mainNav.link.class, href="#" )
        +list-item()( class=mainNav.listItem.class )
          +href( "Кэшбэк" )( class=mainNav.link.class, href="#" )
        +list-item()( class=mainNav.listItem.class )
          +href( "Видео-клиентов" )( class=mainNav.link.class, href="#" )

    //- Номер телефона и кнопка
    +wrapper( "p" )( class=pageHeader.contacts.class )
      +href( "+7 812 344-20-39", "link" )( class=pageHeader.contactsLink.class, href="tel:+78123442039" )
        +icon( "phone", 20, 20 )
      +button( "Обратный звонок", "transparent, border-white" )( class=pageHeader.contactsButton.class )

  //- Контентная часть
  +wrapper()( class=pageHeader.content.class )

    //- Контент
    +wrapper()( class=pageHeader.contentDesc.class )
      +title( "Бесплатный сервис по подбору квартиры в новостройках Санкт-Петербурга без посредников", "h1" )( class=pageHeader.descTitle.class )
      +text( "От застройщиков. Собственников. По переуступкам. Подрядчиков" )( class=pageHeader.descText.class )
      +wrapper( "p" )( class=pageHeader.descButtons.class )
        +button( "Подобрать квартиру", "blue" )( class=pageHeader.descButton.class )
        +button( "Получить кэшбэк", "white" )( class=pageHeader.descButton.class  )

    //- Заметка
    +wrapper()( class=pageHeader.contentNote.class )
      +text( "Наши клиенты покупают квартиры дешевле в среднем <br> на 800 000руб, чем планировали." )( class=pageHeader.noteText.class )
      +button( "Смотрите их истории", "red" )( class=pageHeader.noteButton.class )
        +icon( "play", 11, 16, "play" )

block main
  include ../../blocks/accordion/accordion.pug
  include ../../blocks/slider/slider.pug


  //- Преимущества
  include ../../sections/advantages/advantages.pug
  +section( "section" )( class=advantagesClass )
    +wrapper()( class=advantages.container.class )
      +title( "Преимущества", "h2" )( class=advantages.title.class )
      +list()( class=advantages.list.class )
        +list-item()( class=advantages.listItem.class )
          +title( "78 000 квартир в Спб", "h3" )( class=advantages.subTitle.class )
            +icon( "build", "32", "32" )
          +text( "Мы владеем одной из наиболее крупных межагентских баз недвижимости, " +
          "где размещаются все актуальные квартиры в новостройках" )( class=advantages.text.class )
        +list-item()( class=advantages.listItem.class )
          +title( "Без посредников", "h3" )( class=advantages.subTitle.class )
            +icon( "peoples", "32", "32" )
          +text( "Окончательная коммуникация по стоимости и условиям приобретения " +
          "происходит напрямую между вами и продавцом" )( class=advantages.text.class )
        +list-item()( class=advantages.listItem.class )
          +title( "Кэшбэк до 6%", "h3" )( class=advantages.subTitle.class )
            +icon( "hand", "32", "32" )
          +text( "Окончательная коммуникация по стоимости и условиям приобретения " +
          "происходит напрямую между вами и продавцом" )( class=advantages.text.class )


  //-Презентация
  include ../../sections/presentation/presentation.pug
