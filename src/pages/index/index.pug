    +wrapper()( class=presentation.wrapper.class )
      +wrapper()( class=presentation.container.class )
        +title( "Мы знаем о каждой квартире, которая продается в новостройке", "h2" )( class=presentation.title.class )
        +wrapper( "p" )( class=presentation.buttons.class )
          +button( "Смотреть базу квартир", "blue" )( class=presentation.button.class )
        +wrapper()( class=presentation.body.class )
          +title( "Квартиры в новостройках <br> Санкт-Петербурга", "h3" )( class=presentation.subTitle.class )
